{
  "model": "GATv2 PM2.5 Forecasting GNN - West Java (v3 with persistence)",
  "training_date": "2026-01-31",
  "features": [
    "temperature",
    "humidity",
    "wind_speed",
    "wind_direction",
    "avg_fire_confidence",
    "upwind_fire_count",
    "population_density",
